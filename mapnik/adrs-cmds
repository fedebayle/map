#rm switzerland.osm


rm switzerland.osm
wget http://download.geofabrik.de/osm/europe/switzerland.osm.pbf
DATE="`date +'%F'`"
mv switzerland.osm.pbf switzerland-$DATE.osm.pbf
sudo -u postgres osm2pgsql -p adrs -S err.style -d osm -cs switzerland-$DATE.osm.pbf
./generate_tiles_1024.py
