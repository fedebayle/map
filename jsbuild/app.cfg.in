[app.js]
root = ${vars:jsbuild-root}
first =
    Ext/adapter/ext/ext-base${vars:extdebug}.js
    Ext/ext-all${vars:extdebug}.js
    proj4js/lib/proj4js.js
    proj4js/lib/projCode/merc.js
    proj4js/lib/defs/EPSG900913.js
    OpenLayers/SingleFile.js
    OpenLayers/Util.js
    OpenLayers/BaseTypes.js
    OpenLayers/BaseTypes/Class.js
    OpenLayers/Lang.js
include =
    OpenLayers/Location.js
    OpenLayers/Renderer/VML.js
    OpenLayers/Renderer/SVG.js
    OpenLayers/Control/Navigation.js
    App/main.js
exclude =
    OpenLayers.js
    OpenLayers/Lang/fr.js
    OpenLayers/Lang/en.js

[trace.js]
root = ${vars:jsbuild-root}
first =
#    proj4js/lib/proj4js.js
#    proj4js/lib/projCode/merc.js
#    proj4js/lib/defs/EPSG900913.js
    OpenLayers/SingleFile.js
    OpenLayers/Util.js
    OpenLayers/BaseTypes.js
    OpenLayers/BaseTypes/Class.js
include =
    OpenLayers/Location.js
    OpenLayers/Lang/fr.js
    OpenLayers/Renderer/SVG.js
    OpenLayers/Control/Navigation.js
    App/lamargelle.js
    App/Lang/fr.js
exclude =
    OpenLayers.js

[smallol.js]
root = ${vars:jsbuild-root}
first =
    OpenLayers/SingleFile.js
    OpenLayers/Util.js
    OpenLayers/BaseTypes.js
    OpenLayers/BaseTypes/Class.js
include =
    OpenLayers/Location.js
    OpenLayers/Map.js
    OpenLayers/Control/Navigation.js
    OpenLayers/Control/PanZoomBar.js
    OpenLayers/Control/MousePosition.js
    OpenLayers/Control/Permalink.js
    OpenLayers/Control/Attribution.js
    OpenLayers/Control/ScaleLine.js
    OpenLayers/Layer/TileCache.js
    OpenLayers/Layer/XYZ.js
        
[proj.js]
root = ${vars:jsbuild-root}
first =
    proj4js/lib/proj4js.js
include =
    proj4js/lib/projCode/merc.js
    proj4js/lib/projCode/somerc.js
    proj4js/lib/projCode/tmerc.js
    proj4js/lib/projCode/utm.js
    proj4js/lib/projCode/lcc.js

[en.js]
root = ${vars:jsbuild-root}
first =
    OpenLayers/Lang/en.js
include =
    Ext/src/locale/ext-lang-en.js
    App/Lang/en.js
exclude =
    OpenLayers/Lang.js

[fr.js]
root = ${vars:jsbuild-root}
first =
    OpenLayers/Lang/fr.js
include =
    Ext/src/locale/ext-lang-fr.js
    App/Lang/fr.js
exclude =
    OpenLayers/Lang.js

